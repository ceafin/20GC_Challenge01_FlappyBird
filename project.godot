; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Floppy Fish"
config/description="Floppy Fish, a project for The 20 Games Challenge, Level 1"
run/main_scene="res://scenes/splash_title.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/image="res://assets/dragonfly.png"
boot_splash/fullsize=false
config/icon="res://assets/icon.svg"

[autoload]

EventBus="*res://scripts/event_bus.gd"

[display]

window/size/viewport_width=160
window/size/viewport_height=144
window/size/window_width_override=320
window/size/window_height_override=288
window/stretch/mode="canvas_items"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
